CREATE TABLE "Users" (
  "userID" UUID PK,
  "Password" String,
  "Full Name" String,
  "Address" String,
  "Gender" String,
  "Birth Date" Date,
  "Current Society" String,
  "Address" String,
  "Profile Picture" String,
  "Mobile" integer
);

CREATE TABLE "Membership" (
  "Membership Type" String,
  "Membership Active" Boolean,
  "Membership expiry" Type,
  "userID" UUID FK,
  CONSTRAINT "FK_Membership.userID"
    FOREIGN KEY ("userID")
      REFERENCES "Users"("userID")
);

CREATE TABLE "User's Referral" (
  "userID" FK,
  "Address" String,
  "Invitee of" Array,
  "Invited by" Type,
  "OTP Verified" Boolean
);


